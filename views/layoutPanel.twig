<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="shortcut icon" href="{{ '/css/style/images/favicon.png' | url }}">
    <title>UNIVA | Proyecto Perfil Laboral</title>

    <!-- Bootstrap core CSS -->
    <link href="{{ '/css/style/css/bootstrap.css' | url }}" rel="stylesheet">
    <link href="{{ '/css/style/css/settings.css' | url }}" rel="stylesheet">
    <link href="{{ '/css/style/js/google-code-prettify/prettify.css' | url }}" rel="stylesheet">
    <link href="{{ '/css/style/js/fancybox/jquery.fancybox.css' | url }}" rel="stylesheet" type="text/css" media="all" />
    <link href="{{ '/css/style/js/fancybox/helpers/jquery.fancybox-thumbs.css?v=1.0.2' | url }}" rel="stylesheet" type="text/css" />
    <link href="{{ '/css/style.css' | url }}" rel="stylesheet">
    <link href="{{ '/css/style/css/color/aqua.css' | url }}" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,400italic,500,500italic,700italic,700,900,900italic,300italic,300,100italic,100' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Roboto+Condensed:300italic,400italic,700italic,400,700,300' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Roboto+Slab:400,700,300,100' rel='stylesheet' type='text/css'>
    <link href="{{ '/css/style/type/fontello.css' | url }}" rel="stylesheet">
    <link href="{{ '/css/style/type/budicons.css' | url }}" rel="stylesheet">

    <!-- Particular styles -->
    <style>
        a { color: #5686da; }
        a:hover { text-decoration: underline; color: #f18903; }

        /* Form fields */
        .campos {
            background: transparent !important;
            border: 1px solid white !important;
            width: 100% !important;
        }

        th, input {
          text-align: center;
        }
    </style>

</head>

<body>
  <!-- Container -->
    <div class="body-wrapper">
      <div class="light-wrapper">
        <div class="row">
          <div class="col-sm-12" style="background-color: #002857">

            <div class="row">
                <div class="col-sm-2" align="center">
                  <div class="divide30"></div>
                        <img src="{{ "/css/style/images/univalogo.png" | url }}" class="img-responsive" width="150">
                        <div class="divide50"></div>
                        <h5 style="color: white">{{ user.nombre_usuario }}</h5>
                        <div class="divide30"></div>
                        <div align="center">
                          <a href="{{ "panel" | url }}">
                            <h4 style="color: white"><div class="icon"><i class="icon-chart-bar-1" style="color: #f18903"></i> Inicio</div></h4>
                          </a>

                          <div class="divide30"></div>

                          <a href="{{ "panel/admin/administratives" | url }}">
                            <h4 style="color: white"><div class="icon"><i class="icon-user" style="color: #f18903"></i> Administrativos</div></h4>
                          </a>

                          <div class="divide30"></div>

                          <a href="{{ "panel/admin/reports" | url }}">
                            <h4 style="color: white"><div class="icon"><i class="icon-doc-text" style="color: #f18903"></i> Reportes</div></h4>
                          </a>

                          <div class="divide30"></div>

                          <a href="{{ "panel/admin" | url }}">
                            <h4 style="color: white"><div class="icon"><i class="icon-users" style="color: #f18903"></i> Permisos</div></h4>
                          </a>

                          <div class="divide30"></div>

                          <a href="{{ "admin/profile" | url }}">
                            <h4 style="color: white"><div class="icon"><i class="icon-doc-text" style="color: #f18903"></i> Perfil</div></h4>
                          </a>

                          <div class="divide80"></div>
                          <div class="divide80"></div>

                          <a href="{{ "logout" | url }}">
                            <h4 style="color: white"><div class="icon"><i class="icon-users" style="color: #f18903"></i> Cerrar Sesi√≥n</div></h4>
                          </a>
                        </div>
                </div>


                <div class="col-sm-10"  style="background-color: white" align="center">
                  <!-- Section 1 -->
                    {% block section1 %}
                    {% endblock %}
                  <!-- End Section 1 -->
                </div>

            </div>

          </div>
        </div>
      </div>
    </div>
    <!-- End container -->
    <script src="{{ '/css/style/js/jquery.min.js' | url }}"></script>
    <script src="{{ '/css/style/js/bootstrap.min.js' | url }}"></script>
    <script src="{{ '/css/style/js/twitter-bootstrap-hover-dropdown.min.js' | url }}"></script>
    <script src="{{ '/css/style/js/jquery.themepunch.plugins.min.js' | url }}"></script>
    <script src="{{ '/css/style/js/jquery.themepunch.revolution.min.js' | url }}"></script>
    <script src="{{ '/css/style/js/jquery.fancybox.pack.js' | url }}"></script>
    <script src="{{ '/css/style/js/fancybox/helpers/jquery.fancybox-thumbs.js?v=1.0.2' | url }}"></script>
    <script src="{{ '/css/style/js/fancybox/helpers/jquery.fancybox-media.js?v=1.0.0' | url }}"></script>
    <script src="{{ '/css/style/js/jquery.isotope.min.js' | url }}"></script>
    <script src="{{ '/css/style/js/jquery.easytabs.min.js' | url }}"></script>
    <script src="{{ '/css/style/js/idangerous.swiper-2.1.min.js' | url }}"></script>
    <script src="{{ '/css/style/js/jquery.fitvids.js' | url }}"></script>
    <script src="{{ '/css/style/js/jquery.sticky.js' | url }}"></script>
    <script src="{{ '/css/style/js/google-code-prettify/prettify.js' | url }}"></script>
    <script src="{{ '/css/style/js/jquery.slickforms.js' | url }}"></script>
    <script src="{{ '/css/style/js/instafeed.min.js' | url }}"></script>
    <script src="{{ '/css/style/js/jquery.dcflickr.1.0.js' | url }}"></script>
    <script src="{{ '/css/style/js/retina.js' | url }}"></script>
    <script src="{{ '/css/style/js/classie.js' | url }}"></script>
    <script src="{{ '/css/style/js/scripts.js' | url }}"></script>
</body>

</html>
